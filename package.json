{
  "name": "@gkalpak/cli-utils",
  "version": "0.0.1",
  "description": "A private collection of utilities for developing cli tools.",
  "keywords": [
    "Private",
    "Utility"
  ],
  "main": "lib/index",
  "scripts": {
    "dev": "watch \"npm run test-unit\" ./ --wait 1",
    "lint": "eslint ./",
    "prerelease": "npm test",
    "release": "node bin/run \"npm version \\${*:patch}\"",
    "postrelease": "npm publish",
    "test": "run-s ~~test-pkg lint test-unit test-e2e",
    "test-e2e": "jasmine test/e2e/**/*.spec.js",
    "test-unit": "jasmine test/unit/**/*.spec.js",
    "postversion": "git push && git push --tags",
    "~~test-pkg": "node scripts/test-pkg"
  },
  "bin": {
    "gkcu-expand-cmd": "./bin/expand-cmd.js",
    "gkcu-run": "./bin/run.js"
  },
  "license": "Unlicense",
  "author": "gkalpak <kalpakas.g@gmail.com>",
  "homepage": "https://github.com/gkalpak/cli-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gkalpak/cli-utils.git"
  },
  "dependencies": {
    "chalk": "^2.4.1"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "jasmine": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "watch": "^1.0.2"
  }
}
